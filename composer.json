{
    "name": "fsi/doctrine-extensions",
    "type": "library",
    "description": "FSi Behavioral Doctrine Extensions",
    "keywords": ["fsi", "doctrine-extensions"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lukasz Cybula",
            "email": "lukasz@fsi.pl"
        },
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": ">=5.4",
        "doctrine/common": "~2.4",
        "symfony/property-access": "^2.3|^3.0",
        "symfony/yaml": "^2.2|^3.0",
        "fsi/reflection": "0.9.*",
        "fsi/metadata": "~0.9.2",
        "fsi/property-observer": "~0.9.2",
        "knplabs/gaufrette": "~0.1.4"
    },
    "require-dev": {
        "ext-intl": "*",
        "ext-sqlite3": "*",
        "doctrine/dbal": "~2.4",
        "doctrine/orm": "~2.4",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "FSi\\DoctrineExtensions": "lib/",
            "FSi\\DoctrineExtensions\\Tests": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
